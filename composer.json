{
  "name": "scrumble-nl/laravel-model-ts-type",
  "type": "library",
  "minimum-stability": "dev",
  "homepage": "https://github.com/scrumble-nl/laravel-model-ts-type",
  "description": "This package makes it possible to generate TypeScript types based on your models",
  "keywords": [
    "command",
    "generate",
    "model",
    "TypeScript",
    "type"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Rico Clark",
      "email": "rico@scrumble.nl"
    },
    {
      "name": "Luuk de Weijer",
      "email": "luuk@scrumble.nl"
    }
  ],
  "autoload": {
      "files": [
          "src/Helpers/helpers.php"
      ],
    "psr-4": {"Scrumble\\TypeGenerator\\": "src/"},
    "classmap": [
      "src"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "classmap": [
    ]
  },
  "require": {
    "php": "^8.1",
    "ext-pdo": "*",
    "laravel/framework": "^10.0",
    "laravel/helpers": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6.0 || ^10.0.1",
    "orchestra/testbench": "^8.0",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "replace": {
  },
  "extra": {
    "laravel": {
      "providers": [
        "Scrumble\\TypeGenerator\\ServiceProvider"
      ],
      "aliases": {
        "FormatNamespace": "Scrumble\\TypeGenerator\\Facades\\FormatNamespace"
      }
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php ./vendor/bin/testbench package:discover --ansi"
    ]
  }
}
